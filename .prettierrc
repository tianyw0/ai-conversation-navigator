{
  "trailingComma": "all",
  "semi": true,
  "singleQuote": true,
  "arrowParens": "avoid",
  "printWidth": 120,
  "bracketSameLine": true,
  "htmlWhitespaceSensitivity": "strict",
  "overrides": [
    {
      "files": ["*.tsx", "*.jsx"],
      "options": {
        "jsxSingleQuote": true,
        "jsxBracketSameLine": true
      }
    }
  ]
}
